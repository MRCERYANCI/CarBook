    <script>
        $(function () {
            $('.kartno').click(function () {
                var dg = $(this).attr('id');
                console.log(dg);
                $("#tbody").empty();
                $.ajax({
                    contentType: "application/json",
                    url: "/KargoTakip/karthareketajax?kartno="+dg+"",
                    type: "POST",
      
                    success: function (CardHareketler) {
                        $("#tabloaciklama").css("display", "block")
                        let w = jQuery.parseJSON(CardHareketler)
                        $("#tbody").empty();
                        var tlEntity = " &#8378;";
                        $.each(w, (index, value) => {
                            var checkBox = `<tr><td>${value.Tarih}</td><td>${value.Protype}</td><td>${value.BusNo}</td><td>${value.Amount} ₺</td><td>${value.Balance} ₺</td></tr>`;
                            $(checkBox).appendTo('#tbody');
                        });
                    },
                    error: function (errormessage) {
                        $("#tbody").empty();
                        console.log(errormessage.responseText);
                    }
                });
            });
        })
    </script>

